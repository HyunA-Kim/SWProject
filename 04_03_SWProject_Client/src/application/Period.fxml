<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="160.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Period_Controller">
   <children>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="GAINSBORO" height="160.0" stroke="GAINSBORO" strokeType="OUTSIDE" strokeWidth="2.0" width="300.0" />
      <Label layoutX="15.0" layoutY="20.0" prefHeight="29.0" prefWidth="220.0" text="Please click auto send message cycle">
         <font>
            <Font size="12.0" />
         </font>
      </Label>
      <TextField fx:id="Period_TextField" layoutX="10.0" layoutY="92.0" prefHeight="23.0" prefWidth="62.0">
         <font>
            <Font size="12.0" />
         </font>
      </TextField>
      <Button fx:id="SaveBtn" layoutX="125.0" layoutY="123.0" mnemonicParsing="false" onAction="#moveSave" prefHeight="23.0" prefWidth="69.0" style="-fx-background-color: #ffffff;" text="Running">
         <effect>
            <DropShadow />
         </effect>
         <font>
            <Font size="12.0" />
         </font>
      </Button>
      <Button fx:id="CloseBtn" layoutX="208.0" layoutY="123.0" mnemonicParsing="false" onAction="#moveClose" prefHeight="23.0" prefWidth="69.0" style="-fx-background-color: #ffffff;" text="Close">
         <effect>
            <DropShadow />
         </effect>
         <font>
            <Font size="12.0" />
         </font>
      </Button>
      <Label layoutX="15.0" layoutY="58.0" prefHeight="23.0" prefWidth="57.0" text="Period">
         <font>
            <Font size="12.0" />
         </font>
      </Label>
      <Group layoutX="-2.0" layoutY="6.0" />
      <CheckBox fx:id="Default_CheckBox" layoutX="80.0" layoutY="62.0" mnemonicParsing="false" onAction="#move_Default" text="default">
         <font>
            <Font size="12.0" />
         </font>
      </CheckBox>
      <RadioButton fx:id="Sec_RadioBtn" layoutX="80.0" layoutY="95.0" mnemonicParsing="false" onAction="#move_RadioBtn" text="sec">
         <font>
            <Font size="12.0" />
         </font>
      </RadioButton>
      <RadioButton fx:id="Min_RadioBtn" layoutX="127.0" layoutY="96.0" mnemonicParsing="false" onAction="#move_RadioBtn" text="min">
         <font>
            <Font size="12.0" />
         </font>
      </RadioButton>
      <RadioButton fx:id="Hour_RadioBtn" layoutX="178.0" layoutY="96.0" mnemonicParsing="false" onAction="#move_RadioBtn" text="hour">
         <font>
            <Font size="12.0" />
         </font>
      </RadioButton>
      <RadioButton fx:id="Day_RadioBtn" layoutX="235.0" layoutY="96.0" mnemonicParsing="false" onAction="#move_RadioBtn" text="day">
         <font>
            <Font size="12.0" />
         </font>
      </RadioButton>
   </children>
</AnchorPane>
